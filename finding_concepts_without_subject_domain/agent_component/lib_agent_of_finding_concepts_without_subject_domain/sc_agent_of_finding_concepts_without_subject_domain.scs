sc_agent_of_finding_concepts_without_subject_domain
=> nrel_main_idtf:
	[sc-агент поиска понятия, которые не соотнесены с предметной областью] (* <- lang_ru;; *);
	[sc-agent of finding concepts that are not related to the subject domain] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_concepts_without_subject_domain_condition => ..sc_agent_of_finding_concepts_without_subject_domain_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_finding_concepts_without_subject_domain
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_concepts_without_subject_domain
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_concepts_without_subject_domain;
		proc_of_checking_set_of_semantic_neighbourhood
		};;
		
	-> sc_agent_of_finding_concepts_without_subject_domain_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_concepts_without_subject_domain_condition
= [*
	question_of_finding_semantic_neighborhoods_that_have_not_specified_the_key_sc_element _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_finding_concepts_without_subject_domain_result
= [*
	question_of_finding_semantic_neighborhoods_that_have_not_specified_the_key_sc_element _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
