			_-> rrel_2:: rrel_assign:: rrel_scp_var:: rrel_pos_const_perm:: _temp_arc;;
			_-> rrel_3:: rrel_assign:: rrel_scp_var::  _element;;

			_=> nrel_then:: _..operator_del;;
			_=> nrel_else:: _..operator_add_in_answer;;
			*);;

				_-> _..operator_del
					(*
					_<- eraseEl;;
					
